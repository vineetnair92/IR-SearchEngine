<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>DisplayResults</servlet-name>
        <servlet-class>com.me.servlet.DisplayResults</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>DisplayResults</servlet-name>
        <url-pattern>/DisplayResults</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>WriteToFile</servlet-name>
        <servlet-class>com.me.servlet.WriteToFile</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>WriteToFile</servlet-name>
        <url-pattern>/WriteToFile</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
</web-app>
